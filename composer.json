{
    "name": "sourecode/conventional-changelog",
    "type": "symfony-bundle",
    "description": "Conventional changelog integration into symfony",
    "keywords": [
        "symfony",
        "convention",
        "changelog",
        "git",
        "conventional-changelog"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Schilling",
            "email": "jason@sourecode.dev"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/http-kernel": "^5.3 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "SoureCode\\Bundle\\ConventionalChangelog\\": "./src"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SoureCode\\Bundle\\ConventionalChangelog\\Tests\\": "./tests"
        }
    },
    "minimum-stability": "dev"
}
